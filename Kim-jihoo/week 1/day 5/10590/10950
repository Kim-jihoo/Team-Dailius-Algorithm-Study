import java.util.Scanner;
 
public class Main {
	public static void main(String args[]) {
 
		Scanner sc = new Scanner(System.in);
 
		int C = sc.nextInt();
		int a[] = new int[C];
 
		for (int i = 0; i < C; i++) {
			int A = sc.nextInt();
			int B = sc.nextInt();
			a[i] = A + B;
		}
		
 
		for (int k : a) {
			System.out.println(k);
		}
        sc.close();
	}
 
}
