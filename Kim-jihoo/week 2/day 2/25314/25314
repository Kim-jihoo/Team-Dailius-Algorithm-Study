import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int N = sc.nextInt();
        String st = "";
        sc.close();
        for(int i=1; i<=N/4; i++){
            st = "long " + st;
        }
        System.out.println(st + "int");
    }
}
