import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int [] arr = new int[n];
        int k = 0;
        for(int i=0; i<n; i++){
           arr[i] = sc.nextInt();
           
        }
        int c = sc.nextInt();
        
        for(int j=0; j<n; j++){
            if(c==arr[j]){
                k++;
            }
        }
        System.out.println(k);
    }
}
